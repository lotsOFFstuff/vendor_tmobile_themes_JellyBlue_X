<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Black" parent="@android:style/Theme" />
    <style name="TextAppearance.Holo.Widget.Switch" parent="@android:style/TextAppearance.Holo.Small">
        <item name="android:textColor">#ff33B5E5</item>
    </style>
    <style name="Animation.PopupWindow" parent="@android:style/Animation" />
    <style name="TextAppearance.StatusBar.PhoneTicker" parent="@style/Animation.PopupWindow">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ff33B5E5</item>
    </style>
	<style name="TextAppearance.StatusBar.BatteryTextOnly_Plugged" parent="@android:style/TextAppearance.StatusBar.Icon">
        <!-- Note: must be dp to fit in status bar -->
        <item name="android:textSize">16dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/holo_blue_light</item>
    </style>
	<style name="TextAppearance.StatusBar.Expanded.Clock" parent="@android:style/TextAppearance.StatusBar.Icon">
        <item name="android:textSize">32dp</item>
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/holo_blue_light</item>
    </style>
    <style name="TextAppearance.StatusBar.Expanded.Date" parent="@android:style/TextAppearance.StatusBar.Icon">
        <item name="android:textSize">12dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ff2EA2CC</item>
        <item name="android:textAllCaps">true</item>
    </style>
	<style name="TextAppearance" />
    <style name="TextAppearance.QuickSettings" parent="@style/TextAppearance" />
    <style name="TextAppearance.QuickSettings.TileView" parent="@style/TextAppearance.QuickSettings">
        <item name="android:textSize">12.0dip</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ff33B5E5</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:paddingLeft">6.0dip</item>
        <item name="android:paddingRight">6.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:fadingEdge">horizontal</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">12.0dip</item>
        <item name="android:textAllCaps">true</item>
    </style>
	<style name="Theme.Holo.Light" parent="@*android:style/Theme.Holo">
        <item name="android:textColorPrimary">@color/holo_blue_dark</item>
        <item name="android:textColorSecondary">@color/holo_blue_light</item>
        <item name="android:textColorPrimaryInverse">@color/holo_blue_light</item>
        <item name="android:textColorSecondaryInverse">@color/holo_blue_dark</item>
        <item name="android:popupWindowStyle">@*android:style/Widget.Holo.PopupWindow</item>
        <item name="android:textColor">@color/holo_blue_dark</item>
        <item name="android:popupMenuStyle">@*android:style/Widget.Holo.PopupMenu</item>
        <item name="android:editTextColor">@color/holo_blue_dark</item>
        <item name="android:textCursorDrawable">@drawable/framework_res_text_cursor_holo_light</item>
        <item name="android:widgetCategory">home_screen|keyguard</item>
    </style>
    <style name="Theme.Holo.Light.Dialog" parent="@*android:style/Theme.Holo.Dialog">
        <item name="android:textAppearance">@*android:style/TextAppearance.Holo</item>
        <item name="android:textAppearanceInverse">@*android:style/TextAppearance.Holo.Inverse</item>
        <item name="android:textColorPrimary">@color/holo_blue_light</item>
        <item name="android:textColorSecondary">@color/holo_blue_dark</item>
        <item name="android:editTextColor">@color/holo_blue_light</item>
        <item name="android:textCursorDrawable">@drawable/framework_res_text_cursor_holo_light</item>
    </style>
    <style name="Theme.Holo.Light.DarkActionBar" parent="@style/Theme.Holo.Light.DarkActionBar">
        <item name="android:textColor">@color/holo_blue_light</item>
        <item name="android:actionMenuTextColor">@color/holo_blue_light</item>
    </style>
    <style name="Widget.CompoundButton" parent="@*android:style/Widget.CompoundButton">
        <item name="android:textColor">@color/holo_blue_dark</item>
    </style>
    <style name="TextAppearance.Widget.DropDownHint" parent="@*android:style/TextAppearance.Widget">
        <item name="android:textColor">@color/holo_blue_dark</item>
    </style>
    <style name="TextAppearance.Widget.DropDownItem" parent="@*android:style/TextAppearance.Widget">
        <item name="android:textColor">@color/holo_blue_light</item>
    </style>
    <style name="TextAppearance.Holo.Widget.DropDownHint" parent="@*android:style/TextAppearance.Holo.Widget">
        <item name="android:textColor">@color/holo_blue_light</item>
    </style>
    <style name="TextAppearance.Holo.Widget.DropDownItem" parent="@*android:style/TextAppearance.Holo.Widget">
        <item name="android:textColor">@color/holo_blue_light</item>
    </style>
    <style name="TextAppearance.Holo.Widget.TextView.SpinnerItem" parent="@*android:style/TextAppearance.Holo.Widget.TextView">
        <item name="android:textColor">@color/holo_blue_dark</item>
    </style>
    <style name="TextAppearance.Holo.Widget.EditText" parent="@*android:style/TextAppearance.Holo.Widget">
        <item name="android:textColor">@color/holo_blue_dark</item>
        <item name="android:textColorHint">@color/holo_blue_dark</item>
    </style>
    <style name="TextAppearance.Holo.Widget.PopupMenu" parent="@*android:style/TextAppearance.Holo.Widget.PopupMenu">
        <item name="android:textColor">@color/holo_blue_dark</item>
    </style>
    <style name="TextAppearance.Holo.DialogWindowTitle" parent="@*android:style/TextAppearance.Holo">
        <item name="android:textSize">22.0sp</item>
        <item name="android:textColor">@color/holo_blue_light</item>
    </style>
	<!-- contacts styles -->
    <style name="Theme.QuickContact" parent="@android:style/Theme.Holo">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:listViewStyle">@style/ListViewStyle</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="ListViewStyle" parent="@android:style/Widget.Holo.ListView">
        <item name="android:overScrollMode">always</item>
        <item name="android:background">@drawable/framework_res_background_holo_dark</item>
    </style>
    <style name="ContactsActionBarStyle" parent="@android:style/Widget.Holo.ActionBar.Solid">
        <item name="android:background">@drawable/framework_res_ab_bottom_solid_dark_holo</item>
        <item name="android:displayOptions"></item>
        <item name="android:backgroundStacked">@drawable/framework_res_ab_bottom_solid_dark_holo</item>
    </style>
    <style name="ContactsActionBarDropDownStyle" parent="@android:style/Widget.Holo.Spinner">
        <item name="android:background">@drawable/com_android_contacts_ab_dropdown_navigation_item_background</item>
    </style>
    <style name="ContactsActionBarTabView" parent="@android:style/Widget.Holo.ActionBar.TabView">
        <item name="android:background">@drawable/com_android_contacts_action_bar_tab</item>
    </style>
    <style name="ContactsActionBarTheme" parent="@android:style/Theme.Holo">
        <item name="android:dropDownListViewStyle">@style/ListViewDropdownStyle</item>
        <item name="android:textColor">@android:color/holo_blue_light</item>
        <item name="android:textColorHint">@color/snippet_text_color</item>
        <item name="android:popupMenuStyle">@android:style/Widget.Holo.PopupMenu</item>
    </style>
    <style name="ListViewDropdownStyle" parent="@android:style/Widget.ListView.DropDown">
        <item name="android:listSelector">?android:listChoiceBackgroundIndicator</item>
    </style>
    <style name="ContactsPreferencesTheme" parent="@android:style/Theme.Holo">
        <item name="android:listViewStyle">@style/ListViewStyle</item>
    </style>
    <style name="ContactListFilterTheme" parent="@android:style/Theme.Holo">
        <item name="android:listViewStyle">@style/ListViewStyle</item>
    </style>
    <style name="NonPhoneDialogTheme" parent="@android:style/Theme.Holo.Dialog" />
    <style name="ConfirmAddDetailDialogTheme" parent="@android:style/Theme.Holo.Dialog.MinWidth">
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="ContactEditorAccountsChangedActivityTheme" parent="@android:style/Theme.Holo.Dialog.NoActionBar.MinWidth">
        <item name="android:textColorPrimary">@color/primary_text_color</item>
        <item name="android:textColorSecondary">@color/secondary_text_color</item>
        <item name="android:listViewStyle">@style/ListViewStyle</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="Theme.PhotoSelector" parent="@android:style/Theme.Holo">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="PeopleTheme1" parent="@android:style/Theme.Holo">
        <item name="android:textColorPrimary">@color/primary_text_color</item>
        <item name="android:textColorSecondary">@color/secondary_text_color</item>
        <item name="android:listViewStyle">@style/ListViewStyle</item>
        <item name="android:actionBarStyle">@style/ContactsActionBarStyle</item>
        <item name="android:actionDropDownStyle">@style/ContactsActionBarDropDownStyle</item>
        <item name="android:actionBarTabStyle">@style/ContactsActionBarTabView</item>
        <item name="android:actionBarWidgetTheme">@style/ContactsActionBarTheme</item>
        <item name="android:actionBarItemBackground">@drawable/com_android_contacts_action_bar_item_background</item>
    </style>
<!-- mms styles -->
     <style name="MmsHoloTheme" parent="@android:style/Theme.Holo">
        <item name="android:windowBackground">@color/black</item>
    </style>
    <style name="com_android_mms_WidgetTitle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#ffefefef</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="com_android_mms_NotificationPrimaryText">
        <item name="android:textColor">#ff33B5E5</item>
    </style>
    <style name="com_android_mms_NotificationSecondaryText">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="com_android_mms_NotificationSubjectText">
        <item name="android:textColor">#ffefefef</item>
    </style>
    <style name="deleteThreadTextAppearance" parent="@*android:attr/textAppearanceMedium">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textColor">#ff33B5E5</item>
    </style>
</resources>
